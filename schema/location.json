{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "packet": {
        "$ref": "id.json"
      },
      "time": {
        "type": "number"
      },
      "hash": {
        "$ref": "hash.json"
      },
      "schema_version": {
        "type": "string",
        "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
      }
    },
    "required": ["packet", "time", "hash", "schema_version"],
    "additionalProperties": false
  }
}
