query = { SOI ~ expr ~ EOI }
expr = _{ string | latest }
string = ${ "\"" ~ inner ~ "\"" } // Contents of string including double quotes
inner = @{ char* } // Contents of string without double quotes
char = {
    !("\"" | "\\") ~ ANY // Matches any character except " and \
    | "\\" ~ ("\"" | "\\" ) // Matches \ if followed by " or \ (escape " and \)
}
latest = { "latest" }
WHITESPACE = _{ " " }